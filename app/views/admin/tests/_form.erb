<div class="row">
<%= form_with model: [:admin, @test],local: true do |form| %>
  <%=render 'layouts/shared/errors',resource: @test %>
  <p> <%=form.label :title %>
    <%=form.text_field :title %>
  </p>

  <p> <%=form.label :level %>
    <%=form.number_field :level %>
  </p>
  <p> <%=form.label :category_id %>
    <%=form.collection_select :category_id, Category.all, :id,:title, prompt:true %>
  </p>
    <p> <%=form.label :timer %>
      <%=form.number_field :timer %>
    </p>
  <p><%=form.submit %></p>

<% end %>
</div>
